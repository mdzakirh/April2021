package second.pack;

public class LearningLoop {

	public static void main(String[] args) {

		/*
		 * 
		 * 1. for loop
		 * 2. while loop
		 * 3. do while loop
		 */
		// for ( declaring & initializing, end point of loop, increament/decreament)
		for(int i = 1; i<=10; i+=2) {
			System.out.println(i);
		}
	}

}
